package oops.Questions;

public class Question8th {
    public static void main(String[] args) {
        String name = "Ganesh Dutt";
        String reversed = "";
        for (int i = name.length() - 1; i >= 0; i--) {
            reversed += name.charAt(i);
        }
        System.out.println("Reversed: " + reversed);
    }
}
